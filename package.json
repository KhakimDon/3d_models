{
  "name": "@reidjs/vue3d",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/vue3d.umd.js",
  "module": "./dist/vue3d.js",
  "exports": {
    ".": {
      "import": "./dist/vue3d.js",
      "require": "./dist/vue3d.umd.js"
    }
  },
  "version": "0.0.1",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@google/model-viewer": "3.1.1",
    "three": "0.151.2",
    "pinia": "^2.1.3",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.16.17",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.39.0",
    "eslint-plugin-vue": "^9.11.0",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "typescript": "~5.0.4",
    "vite": "^4.3.9",
    "vitest": "^0.32.0",
    "vue-tsc": "^1.6.5"
  },
  "description": "View 3d models in vue",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reidjs/vue3d.git"
  },
  "keywords": [
    "3d",
    "vue"
  ],
  "author": "reidjs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reidjs/vue3d/issues"
  },
  "homepage": "https://github.com/reidjs/vue3d#readme"
}
